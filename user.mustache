<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="../public/mustache.css" />
    <title>User Cards</title>
  </head>
    <body>
      <div class="user_cards">
        <p class="person">
        {{#users}}
          <img src="{{avatar}}" id="roundAvi" alt="User avatar" />
          <br />
          <h1>{{name}}</h1>
          <br />
          {{address.city}}, {{address.country}} | {{job}}, {{company}}
          <br />
          {{email}} | {{phone}}
          <div class="education">
            <strong><p>Education</p></strong>
            {{university}}
          </div>
          <div class="skills">
            <strong><p>skills</p></strong>
            {{skills}}
          </div>
        {{/users}}
        </p>
      </div>

      <script src="app.js"></script>
    </body>
  </html>
